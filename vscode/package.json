{
    "name": "zendoc",
    "publisher": "zendoc-vs",
    "displayName": "zendoc",
    "description": "semantic wiki syntax",
    "repository": "https://github.com/zen-lang/zd",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.64.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "zd",
            "aliases": ["zendoc", "zd"],
            "extensions": [".zd"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "zd",
            "scopeName": "source.zd",
            "path": "./syntaxes/zd.tmLanguage.json"
        }]
    }
}
